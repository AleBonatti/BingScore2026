{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2022",
    "lib": ["ES2022"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "..",
    "paths": {
      "@/*": ["./*"]
    },
    "outDir": "./.vercel/output/functions"
  },
  "include": ["**/*.ts", "../server/**/*.ts", "../lib/**/*.ts"],
  "exclude": ["node_modules", "../dist", "../src"]
}
