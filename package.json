{
  "name": "bingescore-phase1",
  "version": "1.0.0",
  "description": "BingeScore Phase 1 - Search & Ratings Aggregation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server:dev": "tsx watch server/index.ts",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit"
