    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.10.0",
    "jsdom": "^26.1.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.5",
    "vitest": "^2.1.1"
